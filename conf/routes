# Conversion tasks management
POST    /task                       controllers.CsvToJsonController.createTask
GET     /task                       controllers.CsvToJsonController.listTasks
GET     /task/:taskId               controllers.CsvToJsonController.taskDetails(taskId: String)
DELETE  /task/:taskId               controllers.CsvToJsonController.cancelTask(taskId: String)

GET     /task/result/:name          controllers.CsvToJsonController.taskResult(name: String)

# Check if the app is running
GET     /check                      controllers.CheckController.check

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
